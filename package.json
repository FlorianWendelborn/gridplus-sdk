{
  "name": "gridplus-sdk",
  "version": "0.6.1",
  "description": "SDK to interact with GridPlus Lattice1 device",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src test __tests__",
    "precommit": "npm run lint:fix && npm run test",
    "prepublish": "",
    "postinstall": "",
    "test": "mocha --timeout 120000 -R spec test/testAll.js --recursive --exit",
    "test-eth": "mocha --timeout 180000 -R spec test/testEth.js --recursive --exit",
    "test-btc": "mocha --timeout 180000 -R spec test/testBtc.js --recursive --exit",
    "test-wallet-jobs": "mocha --timeout 300000 -R spec test/testWalletJobs.js --recursive --exit"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GridPlus/gridplus-sdk.git"
  },
  "dependencies": {
    "aes-js": "^3.1.1",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "buffer": "^5.6.0",
    "cbor": "^5.1.0",
    "cbor-js": "^0.1.0",
    "crc-32": "^1.2.0",
    "eip-712": "^0.4.2",
    "elliptic": "6.5.3",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.19",
    "rlp-browser": "^1.0.1",
    "secp256k1": "4.0.2",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "acorn": ">=6.4.1",
    "bip32": "^1.0.4",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "4.0.3",
    "chai": "^4.2.0",
    "cli-interact": "^0.1.9",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethereumjs-tx": "^2.1.2",
    "it-each": "^0.4.0",
    "minimist": ">=0.2.1",
    "mocha": "^5.2.0",
    "random-words": "^1.1.1",
    "readline-sync": "^1.4.9"
  },
  "files": [
    "index.js",
    "src"
  ],
  "license": "MIT"
}
